<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_fuwu_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.gem.mydoctor.fuwu_Activity">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#17abe3"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/toolbar_fuwu"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/image_fuwu"
            app:srcCompat="@mipmap/fanghui_baise"
            android:layout_marginLeft="10dp"
            android:background="@android:color/transparent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的服务"
            android:textColor="#ffffff"
            android:textSize="22dp"
            android:layout_marginLeft="132dp"
            android:layout_alignBottom="@+id/toolbar5"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="13dp" />
    </android.support.v7.widget.Toolbar>
</RelativeLayout>
