<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_qianbao_yue_shuzhimingxi_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.gem.mydoctor.qianbao_yue_shuzhimingxi_Activity">


    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#17abe3"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/yueshouzhimingxi_toolbar"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/shouzhimingxi_fanhui"
            app:srcCompat="@mipmap/fanghui_baise"
            android:background="@android:color/transparent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="余额收支明细"
            android:textColor="#ffffff"
            android:layout_marginLeft="106dp"
            android:textSize="22dp"/>



    </android.support.v7.widget.Toolbar>

    <cn.gem.util.newlistview
        android:layout_below="@+id/yueshouzhimingxi_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/qianbao_shouzhimingxi_newlistview">

    </cn.gem.util.newlistview>

</RelativeLayout>
