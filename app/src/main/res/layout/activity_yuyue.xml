<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_yuyue"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.gem.mydoctor.yuyue_Activity">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#17abe3"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/toolbar_yuye"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/image_yuyue"
            app:srcCompat="@mipmap/fanghui_baise"
            android:background="@android:color/transparent"
            android:layout_marginLeft="10dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的预约"
            android:textColor="#ffffff"
            android:layout_marginLeft="123dp"
            android:textSize="25dp"/>

    </android.support.v7.widget.Toolbar>
    <cn.gem.weight.newlistview
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/yuyue_list"
        android:layout_below="@+id/toolbar_yuye">

    </cn.gem.weight.newlistview>

</RelativeLayout>
